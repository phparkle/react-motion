{
  "name": "react-motion",
  "version": "0.5.2",
  "description": "A spring that solves your animation problems.",
  "main": "lib/react-motion.js",
  "module": "lib/react-motion.js",
  "peerDependencies": {
    "react": "^17.0.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.15.7",
    "@babel/core": "^7.15.8",
    "@babel/plugin-proposal-class-properties": "^7.14.5",
    "@babel/plugin-transform-modules-commonjs": "^7.15.4",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/polyfill": "^7.12.1",
    "@babel/preset-env": "^7.15.8",
    "@babel/preset-flow": "^7.14.5",
    "@babel/preset-react": "^7.14.5",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "codemirror": "^5.63.3",
    "cross-env": "^7.0.3",
    "css-loader": "^6.4.0",
    "eslint": "^8.0.1",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-flowtype": "^6.1.1",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-react": "^7.26.1",
    "flow-bin": "^0.162.1",
    "flow-copy-source": "^2.0.9",
    "husky": "^7.0.2",
    "inject-loader": "^4.0.1",
    "isparta-loader": "^2.0.0",
    "jasmine-core": "^3.10.0",
    "karma": "^6.3.4",
    "karma-chrome-launcher": "^3.1.0",
    "karma-coverage": "^2.0.3",
    "karma-jasmine": "^4.0.1",
    "karma-jasmine-diff-reporter": "^2.0.1",
    "karma-webpack": "^5.0.0",
    "lint-staged": "^11.2.3",
    "lodash.range": "^3.2.0",
    "prettier": "^2.4.1",
    "react": ">=15.5.0",
    "react-codemirror": ">=1.0.0",
    "react-dom": ">=17.0.2",
    "rimraf": "^3.0.2",
    "rollup": "^2.58.0",
    "rollup-plugin-babel": "^4.4.0",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-size-snapshot": "^0.12.0",
    "rollup-plugin-uglify": "^6.0.4",
    "style-loader": "^3.3.0",
    "webpack": "^5.59.0",
    "webpack-command": "^0.5.1",
    "webpack-dev-server": "^4.3.1"
  },
  "scripts": {
    "start": "node server.js",
    "prebuild:dist": "rimraf build",
    "build:dist": "rollup -c",
    "prebuild:lib": "rimraf lib",
    "build:lib": "babel src --out-dir lib",
    "build:flow": "flow-copy-source -v src lib",
    "build": "npm run build:dist && npm run build:lib && npm run build:flow",
    "build-demos": "webpack",
    "lint": "eslint --ext .js,.jsx .",
    "flow_check": "flow check",
    "prepublishOnly": "npm run build",
    "test": "cross-env NODE_ENV=test karma start ./karma.conf.js --single-run",
    "test:travis": "cross-env NODE_ENV=test karma start ./karma.conf.js --single-run",
    "test:dev": "cross-env NODE_ENV=test karma start ./karma.conf.js --no-single-run --auto-watch",
    "test:cov": "cross-env NODE_ENV=test karma start ./karma.conf.js --single-run --reporters coverage",
    "gh-pages": "git fetch origin && git checkout gh-pages && git reset --hard origin/gh-pages && git rebase origin/master --force-rebase && npm run build-demos && git add . && git commit --amend --no-edit && git push origin gh-pages --force && git checkout master",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "prettier": {
    "singleQuote": true,
    "trailingComma": "all"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/chenglou/react-motion.git"
  },
  "keywords": [
    "react",
    "component",
    "react-component",
    "transitiongroup",
    "spring",
    "tween",
    "motion",
    "animation",
    "transition",
    "ui"
  ],
  "author": [
    "nkbt",
    "chenglou"
  ],
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "7.15.4",
    "performance-now": "^2.1.0",
    "prop-types": "^15.7.2",
    "raf": "^3.4.1"
  }
}
